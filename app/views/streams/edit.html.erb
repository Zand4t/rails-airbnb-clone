<h1>Stream Edit</h1>
<p>Should be able to edit the Stream Here</p>

<% @streams = Stream.find(params[:id])%>

<%= form_for(@streams) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :link %>
  <%= f.text_field :link %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>
